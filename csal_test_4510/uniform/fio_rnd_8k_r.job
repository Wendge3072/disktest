[global]
ioengine=libaio
direct=1
iodepth=128
rw=randread
bs=8k
numjobs=1
group_reporting=1
time_based=1
ramp_time=120s
runtime=180s

[job1]
name=job1
filename=/dev/vda

[job2]
name=job2
filename=/dev/vdb

[job3]
name=job3
filename=/dev/vdc

[job4]
name=job4
filename=/dev/vdd

[job5]
name=job5
filename=/dev/vde

[job6]
name=job6
filename=/dev/vdf

[job7]
name=job7
filename=/dev/vdg

[job8]
name=job8
filename=/dev/vdh