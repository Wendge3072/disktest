[global]
ioengine=libaio
direct=1
iodepth=64
rw=randwrite
bs=64k
numjobs=4
size=100%
group_reporting=1

[job1]
name=job1
filename=/dev/vda

[job2]
name=job2
filename=/dev/vdb

[job3]
name=job3
filename=/dev/vdc

[job4]
name=job4
filename=/dev/vdd

[job5]
name=job5
filename=/dev/vde

[job6]
name=job6
filename=/dev/vdf

[job7]
name=job7
filename=/dev/vdg

[job8]
name=job8
filename=/dev/vdh